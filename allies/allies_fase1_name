
getLevelName:	#recebe em a0 o nivel, pega a primeira mensagem
	li t0,0
	beq a0,t0, name.level0
	li t0,1
	beq a0,t0,getLevel.level1
	li t0,2
	beq a0,t0,getLevel.level2
	li t0,3
	beq a0,t0,getLevel.level3
	li t0,4
	beq a0,t0,getLevel.level4
	li t0,5
	beq a0,t0,getLevel.level5
	li t0,5
	beq a0,t0,getLevel.level6
getLevel.level0:
	la a0, fase_0_nome
	ret
getLevel.level1:
	la a0, fase_1_nome
	ret
getLevel.level2:
	la a0, fase_2_nome
	ret
getLevel.level3:
	la a0, fase_3_nome
	ret
getLevel.level4:
	la a0, fase_4_nome
	ret
getLevel.level5:
	la a0, fase_5_nome
	ret
getLevel.level6:
	la a0, fase_6_nome
	ret
